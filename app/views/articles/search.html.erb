
<table>
  <table class="table">
    <h1>Search Results</h1>
    <tr>
      <th>Title</th>
      <th>Article Type</th>
      <th>Online Location</th>
      <th>Description</th>
    </tr>
  </thead>
  <tbody>
    <% @online_articles.each do |online_article| %>
      <tr>

        <td><%=  link_to online_article.Title, online_article %></td>
        <td>Online Article</td>
        <td><a href="<%= online_article.Online_path %>"><%= online_article.Online_path %></a></td>
        <td><%= online_article.Description %></td>

      </tr>
      <% end %>
      <% @articles.each do |article| %>
      <tr>

        <td><%=  link_to article.title, article %></td>
        <td>Local Article</td>
        <td></td>
        <td><%= article.online_location %></td>

      </tr>
    <% end %>
  </tbody>
</table>